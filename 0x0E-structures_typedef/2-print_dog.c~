#include "dog.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * print_dog - print struct dog
 * @d: structure to print
 *
 * Return: no return for this function
 */
void print_dog(struct dog *d)
{
	if (d != NULL)
	{
		if (d->name != NULL)
		{
			printf ("Name: %s\n", d->name);
		}
		else
		{
			printf ("Name: (nil)\n");
		}
		if (d->age != NULL)
		{
			printf ("Name: %f\n", d->age);
		}
		else
		{
			printf("Name: (nil)\n");
		}
		if (d->owner != NULL)
		{
			printf ("Name: %s\n", d->owner);
		}
		else
		{
			printf ("Name: (nil)\n");
		}
	}
}
